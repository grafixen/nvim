{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "scss",
    "html",
    "javascript",
    "typescript",
    "javscriptreact",
    "typescriptreact",
    "typescript.tsx"
  ],

  "prettier.bracketSameLine": true,
  "prettier.bracketSpacing": true,
  "prettier.printWidth": 120,
  "prettier.semi": true,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "all",
  "prettier.singleQuote": true
}
